    top: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom:1px solid #000;
    font-size: 30rpx;
    z-index: 999;
    background-color: #fff;
}
.top .top-list view{
    float: left;
    text-align: center;
