/* pages/other/search/search.wxss */
@import "../../../style/font/icon.wxss";


.history-block{
background: white;
padding-bottom: 20rpx;
}
.history-block .title{
      margin-top: 15rpx;
      width: 100%;
      margin-left: 30rpx;
      height: 80rpx;
      border-bottom: 1rpx solid #eee;
      line-height: 80rpx;
      font-size: 30rpx;
}
.history-block .icon-lajitong{
      float: right;
      margin-right: 80rpx;
       font-size: 35rpx;
      color: #238f46;
}
.search-sign{

      padding-top:10rpx; 
      margin-left: 30rpx;
      width: 100%;
}
.search-sign-item{
line-height: 60rpx;
      display: inline-block;
      background: #eee;
      border-radius: 40rpx;
      padding: 10rpx 20rpx;
      font-size: 30rpx;
      border-bottom: 1rpx solid #eee;
      margin-right: 20rpx;
      margin-top: 15rpx;

}
.search-sign-item::before{
        white-space:nowrap;
}
.search-sign-bar{
      width: 100%;
      height: 100rpx;
}